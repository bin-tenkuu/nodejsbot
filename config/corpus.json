{
	"$schema": "schema/corpus.schema.json",
	"corpora": [
		{
			"name": ".ping",
			"regexp": "^\\.ping$",
			"reply": ".pong!",
			"help": "测试bot是否连接正常"
		}, {
			"name": ".dall1:打开全1模式",
			"regexp": "^\\.dall1$",
			"reply": "[FN:CQBotCOC.setCheater]"
		}, {
			"name": ".dstat",
			"regexp": "^\\.dstat$",
			"reply": "[FN:CQBotCOC.getDiceStat]",
			"help": "查看全部简写"
		}, {
			"name": ".dset <key>[=<value>]",
			"regexp": "^\\.dset +(?<key>\\w[\\w\\d]+)(?:=(?<value>[+\\-*d0-9#]+))?",
			"reply": "[FN:CQBotCOC.getDiceSet]",
			"help": "设置/删除简写"
		}, {
			"name": ".dp <num>",
			"regexp": "^\\.dp *(?<num>\\d+)?",
			"reply": "[FN:CQBotCOC.getAddedRandom]",
			"help": "5分钟之内加投骰"
		}, {
			"name": ".[Dd] <表达式> <...>",
			"regexp": "^\\.[dD] +(?:(?<times>\\d)#)?(?<dice>[^ ]+)",
			"reply": "[FN:CQBotCOC.getDice]",
			"help": "骰子及简单表达式计算"
		}, {
			"name": ".help|帮助",
			"regexp": "^\\.(help|帮助)$",
			"reply": "[FN:CQBot.getHelp]",
			"forward": true
		}, {
			"name": ".色图失败列表",
			"regexp": "^\\.色图失败列表$",
			"reply": "[FN:CQBotPicture.getSetuSet]",
			"forward": true,
			"needAdmin": true
		}, {
			"name": ".插件<other>",
			"regexp": "^\\.插件(?<other>.*)$",
			"reply": "[FN:CQBotPlugin.pluginInfo]",
			"needAdmin": true,
			"forward": true
		}, {
			"name": ".获取<type>列表<other>",
			"regexp": "^\\.获取(?<type>[^ ]+)列表(?<other>.*)$",
			"reply": "[FN:CQBotPlugin.getter]",
			"needAdmin": true,
			"forward": true
		}, {
			"name": ".设置<type> <other>",
			"regexp": "^\\.设置(?<type>[^ ]+) (?<other>.+)$",
			"reply": "[FN:CQBotPlugin.setter]",
			"needAdmin": true
		}, {
			"name": ".<设置|删除>poke<other>",
			"regexp": "^\\.(?<control>设置|删除)poke(?<other>.*)$",
			"reply": "[FN:CQBotPokeGroup.runPrivate]",
			"needAdmin": true,
			"isOpen": false
		}, {
			"name": ".语料库<type><open>",
			"regexp": "^\\.语料库(?<type>私聊|群聊)?(?<open>开|关)$",
			"reply": "[FN:CQBotPlugin.corpusList]",
			"needAdmin": true,
			"forward": true
		}, {
			"name": ".语料库<type><open><nums>",
			"regexp": "^\\.语料库(?<type>私聊|群聊)?(?<open>[开|关])(?<nums>\\s?\\d+)$",
			"reply": "[FN:CQBotPlugin.corpusStat]",
			"needAdmin": true
		}, {
			"name": ".语料库<type><nums>",
			"regexp": "^\\.语料库(?<type>私聊|群聊)?(?<nums>\\s?\\d+)$",
			"reply": "[FN:CQBotPlugin.corpusInfo]",
			"needAdmin": true
		}, {
			"name": "来点图",
			"regexp": "^[来來发發给給][张張个個幅点點份][图圖]片?$",
			"reply": "[FN:CQBotRandomPicture.getRandomPicture]",
			"help": "随机图片",
			"isOpen": false
		}, {
			"name": "来点[<r18>][<key>]色图",
			"regexp": "^[来來发發给給][张張个個幅点點份](?<r18>[Rr]18的?)?(?<keyword>.*)?[色瑟][图圖]$",
			"reply": "[FN:CQBotPicture.getSeTu]",
			"delMSG": 20,
			"help": "来点色图,可选参数:r18,关键字"
		}, {
			"name": "来点东方图",
			"regexp": "^来点东方图$",
			"reply": "[FN:CQBotPicture.getTouHouPNG]",
			"isOpen": false
		}, {
			"name": "看_p站<pid>[-<p>]",
			"regexp": "^看{1,2}p站(?<pid>\\d+)(?:-(?<p>\\d+))?$",
			"reply": "[FN:CQBotPicture.getPixiv]",
			"delMSG": 60,
			"help": "看看p站带上pid发送"
		}, {
			"name": ".股票",
			"regexp": "^\\.(?:gp|股票) ?(?<id>\\d)?$",
			"reply": "[FN:Shares.getSharesPrice]",
			"forward": true,
			"help": "股票游戏:(.gp)或(.股票)+(股票id)查看当前价格,不附带股票id则表示获取所有股票价格",
			"delMSG": 30,
			"isOpen": false
		}, {
			"name": ".股票",
			"regexp": "^\\.(?:gp|股票) ?(?<id>\\d) (?<num>[+-]?\\d+)$",
			"reply": "[FN:Shares.getShares]",
			"forward": true,
			"canGroup": false,
			"help": "股票游戏(私聊):(.gp)或(.股票)+(股票id) (数量)购买或卖出股票.(每买入1,价格+1)",
			"delMSG": 30,
			"isOpen": false
		}, {
			"name": "为什么呢",
			"regexp": "^为什么呢$",
			"reply": "[CQ:reply]是啊，为什么呢，我也在寻找原因呢",
			"canPrivate": false,
			"isOpen": false
		}, {
			"name": "有病吧",
			"regexp": "^有病吧$",
			"reply": "你治得好么?",
			"canPrivate": false,
			"isOpen": false
		}, {
			"name": "太强了",
			"regexp": "^(?:tql|太强了)$",
			"reply": "[FN:CQBotPlugin.getTQL]",
			"canPrivate": false,
			"isOpen": false
		}, {
			"name": ".搜图",
			"regexp": "^\\.搜图$",
			"reply": "[FN:CQBotSearch.getSauceNAO]",
			"help": "带上图片发送",
			"isOpen": false
		}, {
			"name": "(复读)",
			"regexp": "^.+$",
			"reply": "[FN:CQBotRepeat.getRepeat]",
			"canPrivate": false
		}, {
			"name": "(@复读AI)",
			"regexp": "^.*$",
			"reply": "[FN:CQBot.MemeAI]",
			"canPrivate": false
		}, {
			"name": "(保留值)",
			"regexp": "$^",
			"reply": ".",
			"isOpen": false,
			"delMSG": 10,
			"forward": true,
			"needAdmin": true,
			"canPrivate": false,
			"canGroup": false
		}
	]
}