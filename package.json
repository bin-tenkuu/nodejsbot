{
	"name": "nodejsbot",
	"version": "1.0",
	"description": "demo",
	"engines": {
		"node": ">=16"
	},
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"build": "tsc",
		"test": "node -r tsconfig-paths/register.js --inspect",
		"stop": "curl http://127.0.0.1:40000/exit",
		"start": "node -r tsconfig-paths/register.js index.js",
		"ipsort": "node ipsort.js",
		"pm2:start": "pm2 start ecosystem.config.json",
		"pm2:restart": "pm2 restart ecosystem.config.json",
		"pm2:reload": "pm2 reload ecosystem.config.json",
		"pm2:stop": "pm2 stop ecosystem.config.json",
		"pm2:delete": "pm2 delete ecosystem.config.json"
	},
	"keywords": [
		"demo"
	],
	"author": "bin",
	"license": "MIT",
	"devDependencies": {
		"@types/better-sqlite3": "^7.4.1",
		"@types/node": "^16.11.10"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"better-sqlite3": "^7.4.5",
		"go-cqwebsocket": "^6.1.0",
		"log4js": "^6.3.0",
		"node-cache": "^5.1.2",
		"node-expat": "^2.4.0",
		"reflect-metadata": "^0.1.13",
		"tsconfig-paths": "^3.12.0"
	},
	"private": true
}
